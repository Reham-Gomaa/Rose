{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular-monorepo/categories": [
        "libs/categories/src/index.ts"
      ],
      "@angular-monorepo/confirm-dialog": [
        "libs/shared/ui-components/confirm-dialog/src/index.ts"
      ],
      "@angular-monorepo/core": [
        "libs/core/src/index.ts"
      ],
      "@angular-monorepo/notification-toast": [
        "libs/shared/ui-components/notification-toast/src/index.ts"
      ],
      "@angular-monorepo/occasions": [
        "libs/occasions/src/index.ts"
      ],
      "@angular-monorepo/products": [
        "libs/products/src/index.ts"
      ],
      "@angular-monorepo/rose-buttons": [
        "libs/shared/ui-components/rose-buttons/src/index.ts"
      ],
      "@angular-monorepo/rose-custom-inputs": [
        "libs/shared/ui-components/rose-custom-inputs/src/index.ts"
      ],
      "@angular-monorepo/services": [
        "libs/services/src/index.ts"
      ],
      "@rose/address/*": [
        "./apps/rose/src/app/store/address/*"
      ],
      "@rose/checkout/*": [
        "./apps/rose/src/app/store/checkout/*"
      ],
      "@rose/core_directive/*": [
        "./apps/rose/src/app/core/directive/*"
      ],
      "@rose/core_enums/*": [
        "./apps/rose/src/app/core/enums/*"
      ],
      "@rose/core_interfaces/*": [
        "./apps/rose/src/app/core/interfaces/*"
      ],
      "@rose/core_services/*": [
        "./apps/rose/src/app/core/services/*"
      ],
      "@rose/environment/*": [
        "apps/environment/*"
      ],
      "@rose/features_layouts/*": [
        "./apps/rose/src/app/features/layouts/*"
      ],
      "@rose/features_pages/*": [
        "./apps/rose/src/app/features/pages/*"
      ],
      "@rose/features_pages_all_categories/*": [
        "./apps/rose/src/app/features/pages/all-categories/*"
      ],
      "@rose/features_pages_all_categories_components_filter_categories/*": [
        "./apps/rose/src/app/features/pages/all-categories/components/filter-categories/*"
      ],
      "@rose/features_pages_home/*": [
        "./apps/rose/src/app/features/pages/home/*"
      ],
      "@rose/shared_Components_business/*": [
        "./apps/rose/src/app/shared/components/business/*"
      ],
      "@rose/shared_Components_ui/*": [
        "./apps/rose/src/app/shared/components/ui/*"
      ],
      "@rose/shared_services/*": [
        "./apps/rose/src/app/shared/services/*"
      ],
      "@rose/shared_styles/*": [
        "./apps/rose/src/app/shared/styles/*"
      ],
      "@rose/store_auth/*": [
        "./apps/rose/src/app/store/auth/*"
      ],
      "@rose/store_filter/*": [
        "./apps/rose/src/app/store/filter/*"
      ],
      "@rose/store_sort/*": [
        "./apps/rose/src/app/store/sort/*"
      ],
      "@rose_dashboard/core_enums/*": [
        "./apps/rose-dashboard/src/app/core/enums/*"
      ],
      "@rose_dashboard/core_interceptor/*": [
        "./apps/rose-dashboard/src/app/core/interceptor/*"
      ],
      "@rose_dashboard/core_interfaces/*": [
        "./apps/rose-dashboard/src/app/core/interfaces/*"
      ],
      "@rose_dashboard/core_services/*": [
        "./apps/rose-dashboard/src/app/core/services/*"
      ],
      "@rose_dashboard/features_layouts/*": [
        "./apps/rose-dashboard/src/app/features/layout/*"
      ],
      "@rose_dashboard/features_pages/*": [
        "./apps/rose-dashboard/src/app/features/pages/*"
      ],
      "@rose_dashboard/shared_buisness/*": [
        "./apps/rose-dashboard/src/app/shared/buisness/*"
      ],
      "@rose_dashboard/shared_services/*": [
        "./apps/rose-dashboard/src/app/shared/services/*"
      ],
      "@rose_dashboard/shared_styles/*": [
        "./apps/rose-dashboard/src/app/shared/styles/*"
      ],
      "@rose_dashboard/shared_ui/*": [
        "./apps/rose-dashboard/src/app/shared/ui/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
