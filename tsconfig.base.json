{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular-monorepo/categories": [
        "libs/categories/src/index.ts"
      ],
      "@angular-monorepo/products": [
        "libs/products/src/index.ts"
      ],
      "@angular-monorepo/ui": [
        "libs/shared/ui/src/index.ts"
      ],
      "@rose/address/*": [
        "./apps/rose/src/app/store/address/*"
      ],
      "@rose/checkout/*": [
        "./apps/rose/src/app/store/checkout/*"
      ],
      "@rose/core_directive/*": [
        "./apps/rose/src/app/core/directive/*"
      ],
      "@rose/core_enums/*": [
        "./apps/rose/src/app/core/enums/*"
      ],
      "@rose/environment/*": [
        "apps/environment/*"
      ],
      "@rose/core_interfaces/*": [
        "./apps/rose/src/app/core/interfaces/*"
      ],
      "@rose/core_services/*": [
        "./apps/rose/src/app/core/services/*"
      ],
      "@rose/features_layouts/*": [
        "./apps/rose/src/app/features/layouts/*"
      ],
      "@rose/features_pages/*": [
        "./apps/rose/src/app/features/pages/*"
      ],
      "@rose/features_pages_all_categories/*": [
        "./apps/rose/src/app/features/pages/all-categories/*"
      ],
      "@rose/features_pages_all_categories_components_filter_categories/*": [
        "./apps/rose/src/app/features/pages/all-categories/components/filter-categories/*"
      ],
      "@rose/features_pages_home/*": [
        "./apps/rose/src/app/features/pages/home/*"
      ],
      "@rose/shared_Components_business/*": [
        "./apps/rose/src/app/shared/components/business/*"
      ],
      "@rose/shared_Components_ui/*": [
        "./apps/rose/src/app/shared/components/ui/*"
      ],
      "@rose/shared_services/*": [
        "./apps/rose/src/app/shared/services/*"
      ],
      "@rose/shared_styles/*": [
        "./apps/rose/src/app/shared/styles/*"
      ],
      "@rose/store_auth/*": [
        "./apps/rose/src/app/store/auth/*"
      ],
      "@rose/store_filter/*": [
        "./apps/rose/src/app/store/filter/*"
      ],
      "@rose/store_sort/*": [
        "./apps/rose/src/app/store/sort/*"
      ],
      "@rose_dashboard/features_layouts/*": [
        "./apps/rose-dashboard/src/app/features/layout/*"
      ],
      "@rose_dashboard/features_pages/*": [
        "./apps/rose-dashboard/src/app/features/pages/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
