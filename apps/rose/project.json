{
  "name": "rose",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/rose/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/rose",
        "index": "apps/rose/src/index.html",
        "browser": "apps/rose/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/rose/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/rose/public"
          }
        ],
        "styles": [
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeflex/primeflex.css",
          "node_modules/primeflex/themes/primeone-light.css",
          "apps/rose/src/styles.scss"
        ],
        "scripts": [],
        "server": "apps/rose/src/main.server.ts",
        "ssr": {
          "entry": "apps/rose/src/server.ts"
        },
        "outputMode": "server"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "4mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "32kb",
              "maximumError": "40kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/environment/baseurl.dev.ts",
              "with": "apps/environment/baseurl.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/environment/baseurl.prod.ts",
              "with": "apps/environment/baseurl.dev.ts"
            }
          ]
        },
        "analyze-bundle": {
          "sourceMap": true,
          "outputHashing": "none",
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "rose:build:production"
        },
        "development": {
          "buildTarget": "rose:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "rose:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/rose/jest.config.ts"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "rose:build",
        "port": 4200,
        "staticFilePath": "dist/apps/rose/browser",
        "spa": true
      }
    }
  }
}
